{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"plugins": ["./biome/no-import-env.grit"],
	"files": {
		"includes": [
			"src/**/*.ts",
			"src/**/*.tsx",
			"src/**/*.js",
			"src/**/*.jsx",
			"!src/routeTree.gen.ts",
			"package.json",
			"biome.json",
			"vite.config.ts"
		]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true
	}
}
